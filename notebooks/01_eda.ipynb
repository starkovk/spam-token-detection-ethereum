    "print(\"🔵 Starting Data Safety Pipeline...\")\n",
    "\n",
    "# 1. Downsample token_transfers to make it manageable\n",
    "if len(token_transfers) > 500_000:\n",
    "    token_transfers_sample = token_transfers.sample(500_000, random_state=42)\n",
    "    print(f\" token_transfers sampled: {token_transfers_sample.shape}\")\n",
    "else:\n",
    "    token_transfers_sample = token_transfers.copy()\n",
    "    print(f\" token_transfers small enough: {token_transfers_sample.shape}\")\n",
    "\n",
    "# 2. Check null values quickly\n",
    "def check_nulls(df, name):\n",
    "    nulls = df.isnull().mean()\n",
    "    print(f\"🧹 Null check for {name}:\")\n",
    "    print(nulls[nulls > 0].sort_values(ascending=False))\n",
    "    print(\"\\n\")\n",
    "\n",
    "check_nulls(training_tokens, \"training_tokens\")\n",
    "check_nulls(transactions, \"transactions\")\n",
    "check_nulls(token_transfers_sample, \"token_transfers_sample\")\n",
    "check_nulls(nft_transfers, \"nft_transfers\")\n",
    "check_nulls(dex_swaps, \"dex_swaps\")\n",
    "\n",
    "# 3. Optional: Confirm all timestamps are datetime type\n",
    "for df, name in [\n",
    "    (training_tokens, \"training_tokens\"),\n",
    "    (transactions, \"transactions\"),\n",
    "    (token_transfers_sample, \"token_transfers_sample\"),\n",
    "    (nft_transfers, \"nft_transfers\"),\n",
    "    (dex_swaps, \"dex_swaps\")\n",
    "]:\n",
    "    datetime_cols = df.select_dtypes(include=[\"datetime64[ns, UTC]\", \"datetime64[ms, UTC]\"]).columns.tolist()\n",
    "    print(f\" {name} datetime columns:\", datetime_cols)\n",
    "    \n",
    "print(\" Data Safety Pipeline completed!\")\n"
   ]
